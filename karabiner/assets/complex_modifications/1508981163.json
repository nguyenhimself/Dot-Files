{
  "title": "Personal rules (@apm1467)",
  "rules": [
    {
      "description": "Disable command + Q in Safari",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "vk_none"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Safari$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Use right_command + J/K to switch between tabs in applications",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "left_control"
              ]
            }
          ],
          "to_after_key_up": [
            {
              "key_code": "vk_none"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "left_control",
                "left_shift"
              ]
            }
          ],
          "to_after_key_up": [
            {
              "key_code": "vk_none"
            }
          ]
        }
      ]
    },
    {
      "description": "Change right_option to command + option + shift",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_option",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_shift",
              "modifiers": [
                "left_option",
                "left_command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
